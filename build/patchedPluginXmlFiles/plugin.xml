<idea-plugin>
  <id>CleverTap.CleverTap</id>
  <name>CleverTap Assistant</name>
  <vendor email="jidesh@clevertap.com">Clevertap</vendor>
  <description>Plugin helps to integrate Clevertap in your app in less then 5 minutes.
        It helps to install the CleverTap SDK, track your  user events,enrich user profiles,
        send push notifications and see this information within the CleverTap dashboard .</description>
  <idea-version since-build="193.*" />
  <depends>com.intellij.modules.lang</depends>
  <depends>com.intellij.modules.java</depends>
  <depends>org.jetbrains.kotlin</depends>
  <!--    <depends>com.intellij.modules.platform</depends>-->
  <depends>org.jetbrains.android</depends>
  <!--    <depends>com.intellij.modules.androidstudio</depends>-->
  <version>0.4</version>
  <extensions defaultExtensionNs="com.intellij">
    <!--        <notificationGroup id="Display Notification" displayType="BALLOON"/>-->
  </extensions>
  <actions>
    <group id="inner" text="Clevertap Assistant" popup="true" icon="/icons/ctlogo2.png">
      <add-to-group group-id="NewGroup" anchor="after" relative-to-action="NewDir" />
      <action id="CleanPlugin.ID1" class="action.SearchAction" text="Basic Integration" description="ConfigureClevertap." use-shortcut-of="CleanPlugin.ID1" keymap="$default" />
      <action id="CleanPlugin.ID2" class="action.SearchAction_FCM" text="Push Notification" description="ConfigureClevertap." use-shortcut-of="CleanPlugin.ID2" keymap="$default">
        <!--                <keyboard-shortcut keymap="$default" first-keystroke="control alt p"/>-->
      </action>
      <action id="CleanPlugin.ID7" class="action.PushAmp" text="Push Amplification" description="ConfigureCleverTap." keymap="$default" />
      &gt;
      <action id="CleanPlugin.ID4" class="action.XiomiPush" text="Xiaomi Push" description="ConfigureClevertap." keymap="$default" />
      <action id="CleanPlugin.ID5" class="action.HuaweiPush" text="Huawei Push" description="ConfigureClevertap." keymap="$default" />
      <action id="CleanPlugin.ID6" class="action.Geofence" text="Geofence" description="ConfigureClevertap." keymap="$default" />
      <action id="CleanPlugin.ID3" class="action.Auditreport" text="Audit Report" description="ConfigureClevertap." use-shortcut-of="CleanPlugin.ID3" keymap="$default">
        <!-- <keyboard-shortcut keymap="$default" first-keystroke="control alt a"/>-->
      </action>
    </group>
    <action class="action.Auditreport" id="action.Auditreport" text="Audit Report" description="GeneratingAuditReport">
      <add-to-group group-id="GenerateGroup" anchor="first" />
    </action>
    <action class="action.UserEvents" id="action.AccessorAction" text="Clevertap Events" description="AddClevertapEvents/UserProperties">
      <add-to-group group-id="GenerateGroup" anchor="first" />
    </action>
    <action class="action.PushProfile" id="action.AccessorAction_Userproperties" text="Clevertap PushProfile" description="AddClevertapUserProperties">
      <add-to-group group-id="GenerateGroup" anchor="first" />
    </action>
    <action class="action.OnUserLogin" id="action.AccessorAction_Onuserlogin" text="Clevertap OnuserLogin" description="AddClevertapOnuserlogin">
      <add-to-group group-id="GenerateGroup" anchor="first" />
    </action>
    <group id="events" text="Add ClevertTap Events" popup="true" icon="/icons/ctlogo3.png">
      <add-to-group group-id="EditorPopupMenu" anchor="first" />
      <action class="action.UserEvents_cursor" id="event" text="Only Event" description="Demo" />
      <action class="action.UserEventsWithProperties_cursor" id="eventproperties" text="EventsWithProperties" description="Demo" />
    </group>
    <group id="userproperties" text="Add ClevertTap User Properties" popup="true" icon="/icons/ctlogo3.png">
      <add-to-group group-id="EditorPopupMenu" anchor="first" />
      <action class="action.OnUserLogin_cursor" id="onuserlogin" text="OnUserLogin " description="Demo" />
      <action class="action.PushProfile_cursor" id="profilepush" text="Profile Push" description="Demo" />
    </group>
    <group id="pushtemplates" text="Push Templates" popup="true" icon="/icons/ctlogo3.png">
      <add-to-group group-id="EditorPopupMenu" anchor="first" />
      <action class="action.PushTemplates_initalization" id="event1" text="Integration" description="Demo1" />
      <action class="action.Push_Templates" id="eventproperties1" text="Test" description="PushTemplates">
        <!--                    icon="/icons/ctlogo3.png">-->
        <!--                <add-to-group group-id="EditorPopupMenu" anchor="first" />-->
      </action>
      <!--        -->
      <!--            <action class="action.Stop_service"-->
      <!--                    id="stop"-->
      <!--                    text="Stop Service"-->
      <!--                    description="Stop">-->
      <!--            </action>-->
    </group>
    <!--        <group id="pushtemplates_demo" text="Push Templates" popup="true" icon="/icons/ctlogo3.png" >-->
    <!--            <add-to-group group-id="EditorPopupMenu" anchor="first" />-->
    <!--            <action class="action.Push_Templates_temp"-->
    <!--                    id="basic"-->
    <!--                    text="Basic"-->
    <!--                    description="Demo1">-->
    <!--            </action>-->
    <!--            <action class="action.Push_Templates_temp"-->
    <!--                    id="autocarousel"-->
    <!--                    text="Auto Carousel"-->
    <!--                    description="Demo2">-->
    <!--            </action>-->
    <!--            <action class="action.Push_Templates_temp"-->
    <!--                    id="manualcarousel"-->
    <!--                    text="Manual Carousel"-->
    <!--                    description="ManualCarousel">-->
    <!--            </action>-->
    <!--        </group>-->
    <action class="action.DebugLevelCursor" id="debugLevel" text="Debug Level" description="Set clevertap debug level." icon="/icons/ctlogo3.png">
      <add-to-group group-id="EditorPopupMenu" anchor="first" />
    </action>
    <action class="action.IntializeCleverTap_cursor" id="intialize" text="Initialize CleverTap " description="Intialize clevertap object" icon="/icons/ctlogo3.png">
      <add-to-group group-id="EditorPopupMenu" anchor="first" />
    </action>
    <action class="action.SearchAction_FCM" id="action.SearchAction_FCM" text="Add Push Notification with Clevertap" description="Add FCM and Clevertap">
      <add-to-group group-id="GenerateGroup" anchor="first" />
    </action>
    <action class="action.Demo" id="action.Search" text="Push Demo" description="Add FCM and Clevertap">
      <add-to-group group-id="GenerateGroup" anchor="first" />
    </action>
    <!--        <action class="action.start"-->
    <!--                id="action.Search1"-->
    <!--                text="Start Demo"-->
    <!--                description="StartDemo">-->
    <!--            <add-to-group group-id="GenerateGroup" anchor="first"/>-->
    <!--        </action>-->
  </actions>
  <project-components>
    <component>
      <implementation-class>ui.ObjectGraph</implementation-class>
    </component>
  </project-components>
</idea-plugin>
